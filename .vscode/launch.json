{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Wails: Production raycast",
      "type": "go",
      "request": "launch",
      "mode": "exec",
      "program": "${workspaceFolder}/build/bin/raycast",
      "preLaunchTask": "build",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Wails: Debug raycast",
      "type": "go",
      "request": "launch",
      "mode": "exec",
      "program": "${workspaceFolder}/build/bin/raycast",
      "preLaunchTask": "build debug",
      "cwd": "${workspaceFolder}",
      "env": {
        "RAYCAST_COMMAND_DIR": "${workspaceFolder}/script_commands"
      }
    },
    {
      "name": "Wails: Dev raycast",
      "type": "go",
      "request": "launch",
      "mode": "exec",
      "program": "${workspaceFolder}/build/bin/raycast",
      "preLaunchTask": "build dev",
      "cwd": "${workspaceFolder}"
    }
  ]
}
